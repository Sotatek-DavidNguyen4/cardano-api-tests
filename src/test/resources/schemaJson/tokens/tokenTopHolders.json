{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "address": {
              "type": "string"
            },
            "quantity": {
              "type": "integer"
            }
          },
          "required": [
            "address",
            "quantity"
          ]
        }
      ]
    },
    "totalItems": {
      "type": "integer"
    },
    "totalPages": {
      "type": "integer"
    },
    "currentPage": {
    }
  },
  "required": [
    "currentPage",
    "data",
    "totalItems",
    "totalPages"
  ]
}
